nums1=[9,1,4,5,4,0]
nums2=[2,5,8,6,9]
answer=[]
a1=0
a2=0
for i in range(0,len(nums1)):
    for j in range(0,len(nums2)):
        if(nums1[i]==nums2[j]):
            if(i>=a1):
                a1=i
                if(j>=a2):
                    a2=j
answer.append(a1)
answer.append(a2)
print(answer)
            
